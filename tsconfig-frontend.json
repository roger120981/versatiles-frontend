{
	"compilerOptions": {
		"lib": [
			"dom"
		],
		"module": "ESNext",
		"moduleResolution": "node",
		"target": "ES2020",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"declaration": true,
		"allowJs": true,
		"noEmit": true,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true
	},
	"include": [
		"frontend/**/*.ts",
	]
}